{
  "extends": "eslint-config-egg/typescript",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "space-before-function-paren": "off",
    "comma-dangle": "off",
    "array-bracket-spacing": "off",
    "no-trailing-spaces": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}
